generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Gift {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  message   String?
  elderName String
  createdAt DateTime @default(now())
}
